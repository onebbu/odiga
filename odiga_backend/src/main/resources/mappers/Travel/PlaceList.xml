<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.odiga.mytrip.travel.dao.PlaceDAO">


    <!-- 여행지 지역별 리스트 가져오기 -->
    <select id="getPlaceList" resultType="com.odiga.mytrip.travel.vo.TravelListVO">

        SELECT *
        FROM TravelList
        WHERE areacode = #{areacode} and ROWNUM = 2

    </select>
</mapper>
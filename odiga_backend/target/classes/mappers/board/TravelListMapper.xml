<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.odiga.mytrip.dao.TravelDAO">

    <!-- 여행지 정보 가져오기 -->
    <select id="getTravelInfo" resultType="com.odiga.mytrip.api.ApiVO">
        SELECT *
        FROM TravelList
        WHERE contentId = #{contentId}
    </select>

    

</mapper>
